

  <%= simple_form_for(@dream) do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name, placeholder: "Select a name" %>
    </div>

     <div>
    <%= f.label :description %>
    <%= f.text_field :description, placeholder: "Select a description" %>
     </div>

     <div>
      <%= f.label :price %>
      <%= f.text_field :price, placeholder: "Which price ?" %>
     </div>

     <div>
      <%= f.label :transpiration_level %>
      <%= f.text_field :transpiration_level %>
     </div>

     <div>
      <%= f.label :category %>
      <%= f.text_field :category%>
     </div>

     <div>
       <%= f.input :photo, as: :file %>
     </div>


    <%= f.submit %>
  <% end %>





