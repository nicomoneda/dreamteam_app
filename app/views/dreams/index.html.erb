<h1>My dreams</h1>

<ul>
    <% @dreams.each do |dream| %>
        <li>
            <%= dream.name %>
            <%= dream.description %>
            <%= dream.price %>
            <%= dream.transpiration_level %>
            <%= dream.category %>
            <img src="<% dream.poster_url %>" alt="poster_url">
        </li>
        <%= link_to 'Show', dream_path(dream) %>
        <%= link_to 'Edit', edit_dream_path(dream) %>
        <%= link_to 'Destroy', dream, method: :delete, data: { confirm: 'Are you sure?' } %>
    <% end %>
</ul>
